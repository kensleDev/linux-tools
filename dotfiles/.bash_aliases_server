# -----------------------------------------NGINX
alias svrs='sudo service start nginx'
alias svrss='sudo service stop nginx'
alias svrrs='sudo service restart nginx'
alias svrrl='sudo service reload nginx'
# -----------------------------------------PM2
alias pms='pm2 start'
alias pmss='pm2 stop'
alias pmr='pm2 restart'
alias pml='pm2 list'
alias pmll='pm2 prettylist'
alias pl='pm2 logs'
alias pmd='pm2 delete'
alias port='lsof -i :'
